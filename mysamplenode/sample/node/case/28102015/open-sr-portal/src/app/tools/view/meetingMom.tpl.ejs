<style type="text/css">
 .form-control-feedback {
    width: 58px !important;
}
</style>
<section class="content-header">
  <h1> Meeting & Mom  </h1>          
</section>


<section class="content">
  <div id="mommeetingPage" class="box box-default" ng-init="getAllUser(); Datainit('getMOMmeeting','i');">
    <div class="box-body boxdefault" id="accordion">
      <div class="panel">
        <div class="row collapse" id="createMom">
          <ng-include src="'tools/view/createMom.tpl.ejs'"></ng-include>
        </div>
      </div>
      <div class="panel">
        <div class="row collapse" id="momView">
          <ng-include src="'tools/view/meetingView.tpl.ejs'"></ng-include>    
        </div>
      </div>
      <div class="panel">
        <div class="row collapse in" id="momList">
          <ng-include src="'tools/view/meetingList.tpl.ejs'"></ng-include>    
        </div>
      </div>
    </div>
  </div>
</section>
